{
  "source": {
    "include": ["src", "src/structures", "src/structures/typedefs"],
    "exclude": ["./node_modules", "./tests", "./.vscode"]
  },
  "opts": {
    "destination": "./docs",
    "template": "jsdoc-template"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "referenceTitle": "@darkgoatie/Mangadex-api",
    "disableSort": false,
    "collapse": false
  }
}
